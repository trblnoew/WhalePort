<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%>
<link rel="stylesheet" href="${pageContext.request.contextPath}/resources/assets/libs/quill/dist/quill.snow.css">
<script src="${pageContext.request.contextPath}/resources/assets/libs/quill/dist/quill.js"></script>

<style>
    .custom-height {
        height: 250px; 
        display: flex;
        align-items: center; 
        justify-content: center; 
    }
    table {
        margin: 10px;
    }
    .card {
        width: 400px;
        margin: 10px;
    }
    .chat-container {
        display: flex;
        margin-top: 60px;
    }
    .chat-banner {
        width: 25%;
        height: 70vh;
        position: fixed;
        bottom: 5vh;
        left: 3vh;
        border: 1px solid #ccc;
        border-radius: 10px;
        padding: 20px;
        margin-bottom: 25px;
        background-image: url('${pageContext.request.contextPath}/resources/assets/images/chat/chat.png');
        box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
    }
    .chat-content {
        margin-left: 35%;
        width: 65%;
        padding: 20px;
    }
    #AIChatbox {
        width: 100%;
        height: 70%;
        border: 1px solid #ccc;
        border-radius: 5px;
        padding: 10px;
        background-color: #fff;
        overflow-y: auto;
        overflow-x: hidden;
        margin-top: 80px;
    }
    /* 스크롤바 스타일 */
    #AIChatbox::-webkit-scrollbar {
        width: 8px;
    }
    #AIChatbox::-webkit-scrollbar-thumb {
        background-color: #888;
        border-radius: 10px;
    }
    #AIChatbox::-webkit-scrollbar-thumb:hover {
        background-color: #555;
    }
    #AIChatbox::-webkit-scrollbar-track {
        background: #f1f1f1;
        border-radius: 10px;
    }
    @media screen and (max-width: 768px) {
        .chat-banner {
            width: 100%;
            height: auto;
            position: relative;
            top: 0;
            left: 0;
        }
        .chat-content {
            margin-left: 0;
            width: 100%;
        }
    }
    #linkBtn {
        width: 830px;
        line-height: 1px;
        cursor: pointer;
        height: 60px;
    }
    #linkBtnEx {
        font-size: 25px;
        margin-right: 5px;
    }
    .restrictCard {
        display: flex;
    }
    .restrictBlueWord {
        font-size: 17px;
        color: teal;
    }
    .restrictExWord {
        margin-left: 25px;
    }
    .message-container {
        display: flex;
        margin: 10px 0;
    }
    .user-message {
        background-color: #e2f7e5;
        border-radius: 10px 10px 0 10px;
        padding: 10px;
        max-width: 60%;
        margin-left: auto;
        word-wrap: break-word;
    }
    .response-message {
        background-color: #cce8fb;
        border-radius: 10px 10px 10px 0;
        padding: 10px;
        max-width: 80%;
        margin-right: auto;
        word-wrap: break-word;
    }
</style>
