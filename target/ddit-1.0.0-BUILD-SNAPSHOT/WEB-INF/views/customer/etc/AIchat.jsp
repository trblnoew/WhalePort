<script type="text/javascript">
    $(function(){
        var chatBox = $("#AIChatbox");
        var userInput = $("#userQueInput");

        // 질문 전송 함수
        function sendQuestion() {
            var question = userInput.val();
            if (question.trim() === "") return; // 빈 질문은 무시

            // 사용자 질문 표시
            var chatStr = "<div class='message-container'>";
            chatStr +=   "<div class='user-message'>"+question+"</div>";
            chatStr += "</div>";
            $('#msg').append(chatStr);
            userInput.val(""); // 입력 창 초기화
            chatBox.scrollTop(chatBox[0].scrollHeight);

            $.ajax({
                url: "/chat",
                type: "post",
                data: {message: question},
                success: function(result){
                    // 대답 표시
                    var responseStr = "<div class='message-container'>";
                    responseStr +=   "<div class='response-message'>"+result+"</div>";
                    responseStr += "</div>";

                    $('#msg').append(responseStr);
                    chatBox.scrollTop(chatBox[0].scrollHeight); // 스크롤 이동
                },
                error: function(){
                    var errorStr = "<div class='message-container'>";
                    errorStr +=   "<div class='response-message'>죄송합니다. 서버 오류로 답변이 불가능합니다.</div>";
                    errorStr += "</div>";
                    $('#msg').append(errorStr);
                    chatBox.scrollTop(chatBox[0].scrollHeight); // 스크롤 이동
                }
            });
        }

        // 폼 제출 이벤트 처리
        $("#questionForm").on("submit", function(event){
            event.preventDefault(); // 폼의 기본 동작 중지
            sendQuestion();
        });
    });
</script>
